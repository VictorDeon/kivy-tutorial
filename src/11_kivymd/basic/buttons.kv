MyInterface:
<MyInterface>:
    MDIconButton:
        icon: "language-python"
        theme_text_color: "Custom"
        text_color: app.theme_cls.primary_color
        pos: dp(100), dp(300)
    MDFloatingActionButton:
        icon: "android"
        md_bg_color: 1,0,0,1
        pos: dp(200), dp(300)
    MDFlatButton:
        text: "Classic Button"
        pos: dp(300), dp(300)
    MDRoundFlatIconButton:
        icon: "leaf"
        text: "Rounded Flat Icon Button"
        icon_color: 0,.3,.7,1
        text_color: 0,.2,.8,1
        line_color: 0,0,1,1
        pos: dp(450), dp(300)